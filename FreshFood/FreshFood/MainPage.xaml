             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="clr-namespace:FreshFood.Views"
             mc:Ignorable="d"
             x:Class="FreshFood.MainPage"
